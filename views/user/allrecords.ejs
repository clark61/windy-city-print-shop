<% if (allrecs.length > 0) {%>
    <table class="db-table">
            <tr>
                <th>ID</th>
                <th>User Name</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
                <th>Address 1</th>
                <th>Address 2</th>
                <th>City</th>
                <th>State</th>
                <th>Zip</th>
                <th colspan="3">Actions</th>
            </tr>
        <% allrecs.forEach((recref, index) => { %>
            <tr>
                <td><%= recref.id %></td>
                <td><%= recref.user_name %></td>
                <td><%= recref.first_name %></td>
                <td><%= recref.last_name %></td>
                <td><%= recref.email %></td>
                <td><%= recref.address_1 %></td>
                <td><%= recref.address_2 %></td>
                <td><%= recref.city %></td>
                <td><%= recref.state %></td>
                <td><%= recref.zip %></td>
                <td><a href='/user/<%= recref.id %>/show'>Details</a></td>
                <td><a href='/user/<%= recref.id %>/edit'>Edit</a></td>
                <td><a href='/user/<%= recref.id %>/delete'onclick="return confirm('Are you sure you want to delete this record?');">Delete</a></td>
            </tr>
        <% }); %>
    </table>
<% } else { %>
    <p>Not Records Available</p>
<% } %>
<p> <a class="default-button" href='/user/addrecord'>Add</a> </p>

