<h1>Customer Listing</h1>
<% if (allrecs.length > 0) {%>
    <table class="db-table">
            <tr>
                <th>ID</th>
                <th>User Name</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
                <th>Address 1</th>
                <th>Address 2</th>
                <th>City</th>
                <th>State</th>
                <th>Zip</th>
            </tr>
        <% allrecs.forEach((recref, index) => { %>
            <tr>
                <td><%= recref.id %></td>
                <td><%= recref.user_name %></td>
                <td><%= recref.first_name %></td>
                <td><%= recref.last_name %></td>
                <td><%= recref.email %></td>
                <td><%= recref.address_1 %></td>
                <td><%= recref.address_2 %></td>
                <td><%= recref.city %></td>
                <td><%= recref.state %></td>
                <td><%= recref.zip %></td>
            </tr>
        <% }); %>
    </table>
<% } else { %>
    <p>No Records Available</p>
<% } %>
